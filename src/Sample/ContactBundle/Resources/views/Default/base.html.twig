<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}お問い合わせ{% endblock %}</title>
    {% block stylesheets %}
        <link href="{{ asset('css/styles.css') }}" rel="stylesheet" type="text/css" />
    {% endblock %}
    {% block javascripts %}
        <link href="{{ asset('js/scripts.css') }}" rel="stylesheet" type="text/css" />
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

</head>
<body>

{% block header %}
<div id="header">
        <ul>
            <li><a href="/">ホーム</a></li>
            <li><a href="/blog">ブログ</a></li>
        </ul>
</div>
{% endblock %}

{% block body %}{% endblock %}
</body>
</html>
